@{
    ViewBag.Title = "Home Page";
}
<div id="dashboard">

    <div class="jumbotron" style="text-align: center; background-color: #003300 !important; color: white !important">
        <h1>NUEVA VIZCAYA</h1>
        <h1></h1>
        <h1 style="color: red"> COVID - 19</h1>
        <h1> TRACKER</h1>
        <h2>Data as of @DateTime.Now.ToString("MMM dd, yyyy hh:mm tt")  </h2>
    </div>

    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">
            @Html.Action("PUIPartial")
        </div>
    </div>
    <div class="row">
        <div class="col-lg-6 col-md-6 col-sm-6">
            @Html.Action("AwaitingPartial")
        </div>

        <div class="col-lg-6 col-md-6 col-sm-6">
            @Html.Action("NegativePartial")
        </div>
    </div>
    <div class="row">
        <div class="col-lg-6 col-md-6 col-sm-6">
            @Html.Action("ConfirmedPartial")
        </div>

        <div class="col-lg-6 col-md-6 col-sm-6">
            @Html.Action("DeathsPartial")
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <div id="patients">
                @Html.Action("PatientsGridViewPartial", "Admin")
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <div class="well well-sm">
                <span class="pull-left" hidden>
                    Para sa katanungan at karagdagang impormasyon
                    <b>0917-659-6959</b>
                </span>
                <span class="pull-right">

                    Disclaimer: Information listed in this web page may change at any time due to evolving situation.
                    <br />
                    <br />

                    <p style="text-align: right; font-size:10px">Developed by the Provincial Information Technology Division, Governor's Office </p>
                </span>

                <span class="clearfix">

                </span>
            </div>
        </div>
    </div>

</div>